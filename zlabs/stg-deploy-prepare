#!/bin/bash
function help {
      echo "Opens google chrome with tab to create deploy PRs "
      echo " ? --help print this help"	
}


for ((i=1;i<=$#;i++));
do
  parameter=${!i}
  case $parameter in
    [?] | --help )
      help
      exit 2
      ;;
    * )
      echo "Invalid argument: ${!i}"
      help
      exit 2
      ;;
  esac
done;

google-chrome --profile-directory=Default --new-window 'http://stash.aur.ziprealty.com/dashboard' 'http://stash.aur.ziprealty.com/projects/DP/repos/nifi-zap/pull-requests?create&targetBranch=refs%2Fheads%2Fstage&sourceBranch=refs%2Fheads%2Fdevelopment&targetRepoId=450' 'http://stash.aur.ziprealty.com/projects/DP/repos/mls-data-platform-sync/pull-requests?create&targetBranch=refs%2Fheads%2Fstage&sourceBranch=refs%2Fheads%2Fdevelopment&targetRepoId=916' 'http://stash.aur.ziprealty.com/projects/DP/repos/mls-data-platform/pull-requests?create&targetBranch=refs%2Fheads%2Fstage&sourceBranch=refs%2Fheads%2Fdevelopment&targetRepoId=915' 'http://stash.aur.ziprealty.com/projects/DP/repos/mls-dataplatform-mongoconnector/pull-requests?create&targetBranch=refs%2Fheads%2Fstage&sourceBranch=refs%2Fheads%2Fdevelopment&targetRepoId=1716' 'http://stash.aur.ziprealty.com/projects/DP/repos/mls-dataplatform-transformations/pull-requests?create&targetBranch=refs%2Fheads%2Fstage&sourceBranch=refs%2Fheads%2Fdevelopment&targetRepoId=1030' 'http://stash.aur.ziprealty.com/projects/DP/repos/mls_data_platform_web_ui/pull-requests?create&targetBranch=refs%2Fheads%2Fstage&sourceBranch=refs%2Fheads%2Fdevelopment&targetRepoId=951'

exit 0
